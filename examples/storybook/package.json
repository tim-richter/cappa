{
  "name": "@cappa/example-storybook",
  "version": "0.0.29",
  "private": true,
  "license": "MIT",
  "author": "Tim Richter",
  "type": "module",
  "scripts": {
    "screenshot": "cappa capture -l 5",
    "screenshot:review": "cappa review -l 5",
    "screenshot:approve": "cappa approve -l 5",
    "screenshot:status": "cappa status",
    "screenshot:test": "pnpm build-storybook && start-server-and-test 'serve-storybook' 8080 'screenshot'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "serve-storybook": "pnpx http-server -p 8080 storybook-static -s"
  },
  "dependencies": {
    "@cappa/cli": "workspace:*",
    "@cappa/core": "workspace:*",
    "@cappa/plugin-storybook": "workspace:*"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^9.1.12",
    "@storybook/addon-docs": "^9.1.12",
    "@storybook/addon-vitest": "^9.1.12",
    "@storybook/react-vite": "^10.2.12",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "catalog:",
    "playwright": "1.55.1",
    "start-server-and-test": "^2.1.3",
    "storybook": "10.2.12"
  }
}
